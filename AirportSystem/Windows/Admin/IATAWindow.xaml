<Window x:Class="AirportSystem.IATAWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="IATA" Height="250" Width="250"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="160*"/>
            <RowDefinition Height="59*"/>
        </Grid.RowDefinitions>
        <ListBox>
            <ListBoxItem>SU - Авиакомпания "Аерофлот"</ListBoxItem>
            <ListBoxItem>UT - Авиакомпания "UTAir"</ListBoxItem>
            <ListBoxItem>U6 - Авиакомпания "Ural Airlines"</ListBoxItem>
            <ListBoxItem>ZF - Авиакомпания "Azure Air"</ListBoxItem>
            <ListBoxItem>B2 - Авиакомпания "Belavia"</ListBoxItem>
            <ListBoxItem>BT - Авиакомпания "Baltic Air"</ListBoxItem>
        </ListBox>
        <Button Grid.Row="1" Content="Закрыть" Click="Button_Click"/>
    </Grid>
</Window>
